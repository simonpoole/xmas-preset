<?xml version="1.0" encoding="UTF-8"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
	shortdescription="Xmas Preset"
	description="Xmas preset for Vespucci and JOSM, based on the original JOSM version from Jan Tappenbeck">
	<!-- Icons have been derived from mainly from http://osm-icons.org/ Format 
		description: https://josm.openstreetmap.de/wiki/TaggingPresets -->
	<!-- Groups -->
	<group name="Christmas" de.name="Weihnachten" icon="${ICONPATH}xmas-logo.${ICONTYPE}">
		<item name="Christmas tree" de.name="Weihnachtsbaum" icon="${ICONPATH}tree.${ICONTYPE}">
			<link href="http://wiki.openstreetmap.org/wiki/Key:xmas:feature"
				de.href="http://wiki.openstreetmap.org/wiki/DE:Key:xmas:feature"
				fr.href="http://wiki.openstreetmap.org/wiki/FR:Key:xmas:feature" />
			<!-- Feste Werte -->
			<key key="xmas:feature" value="tree" />
			<!-- Variable Werte -->
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true" />
		</item>
		<item name="Christmas market" de.name="Weihnachtsmarkt" icon="${ICONPATH}market.${ICONTYPE}">
			<link href="http://wiki.openstreetmap.org/wiki/DE:Tag:xmas" />
			<!-- Feste Werte -->
			<key key="xmas:feature" value="market" />
			<!-- Variable Werte -->
			<text key="xmas:name" text="Name" default="" delete_if_empty="true" />
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true" />
			<text key="xmas:day_date" text="Date range" de.text="Datumsbereich" default="" delete_if_empty="true" />
			<text key="xmas:opening_hours" text="Opening hours (xmas)" de.text="Öffnungszeiten (xmas)" default="" delete_if_empty="true" />
			<text key="opening_hours" text="Opening hours" de.text="Öffnungszeiten" default="" delete_if_empty="true" />
			<text key="xmas:url" text="Website" default="" delete_if_empty="true" />
			<text key="xmas:note" text="Note" default="" delete_if_empty="true" />
		</item>
		<item name="Christmas event" de.name="Weihnachtsanlass" icon="${ICONPATH}event.${ICONTYPE}">
			<link href="http://wiki.openstreetmap.org/wiki/Key:xmas:feature"
				de.href="http://wiki.openstreetmap.org/wiki/DE:Key:xmas:feature"
				fr.href="http://wiki.openstreetmap.org/wiki/FR:Key:xmas:feature" />
			<!-- Feste Werte -->
			<key key="xmas:feature" value="event" />
			<!-- Variable Werte -->
			<text key="xmas:name" text="Name" default="" delete_if_empty="true" />
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true" />
			<text key="xmas:day_date" text="Date range" de.text="Datumsbereich" default="" delete_if_empty="true" />
			<text key="xmas:opening_hours" text="Opening hours (xmas)" de.text="Öffnungszeiten (xmas)" default="" delete_if_empty="true" />
			<text key="opening_hours" text="Opening hours" de.text="Öffnungszeiten" default="" delete_if_empty="true" />
			<text key="xmas:url" text="Website" default="" delete_if_empty="true" />
			<text key="xmas:note" text="Note" default="" delete_if_empty="true" />
		</item>
		<item name="Christmas pyramid" de.name="Weihnachtspyramide" icon="${ICONPATH}pyramid.${ICONTYPE}">
			<link href="http://wiki.openstreetmap.org/wiki/Key:xmas:feature"
				de.href="http://wiki.openstreetmap.org/wiki/DE:Key:xmas:feature"
				fr.href="http://wiki.openstreetmap.org/wiki/FR:Key:xmas:feature" />
			<!-- Feste Werte -->
			<key key="xmas:feature" value="pyramid" />
			<!-- Variable Werte -->
			<text key="xmas:name" text="Name" default="" delete_if_empty="true" />
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true" />
			<text key="xmas:day_date" text="Date range" de.text="Datumsbereich" default="" delete_if_empty="true" />
			<text key="xmas:note" text="Note" de.text="Hinweis" default="" delete_if_empty="true" />
		</item>
	</group>
</presets>
